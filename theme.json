{
  "version": 3,
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "settings": {
    "color": {
      "defaultPalette": false,
      "defaultGradients": false,
      "defaultDuotone": false,
      "palette": [
        {
          "slug": "black",
          "color": "#000000",
          "name": "Black"
        },
        {
          "slug": "white",
          "color": "#FFFFFF",
          "name": "White"
        },
        {
          "slug": "dark",
          "color": "#121212",
          "name": "Dark"
        },
        {
          "slug": "light",
          "color": "#FFF7E9",
          "name": "Light"
        },
        {
          "slug": "primary",
          "color": "#AE8F55",
          "name": "Primary"
        },
        {
          "slug": "secondary",
          "color": "#FFEECE",
          "name": "Secondary"
        },
        {
          "slug": "success",
          "color": "green",
          "name": "Success"
        },
        {
          "slug": "warning",
          "color": "yellow",
          "name": "Warning"
        },
        {
          "slug": "error",
          "color": "red",
          "name": "Error"
        }
      ],
      "gradients": [],
      "duotone": []
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "'Inter', sans-serif",
          "slug": "primary",
          "name": "Primary"
        },
        {
          "fontFamily": "'Inter', sans-serif",
          "slug": "secondary",
          "name": "Secondary"
        }
      ],
      "fontSizes": [
        {
          "slug": "xs",
          "size": "0.75rem",
          "name": "Extra Small"
        },
        {
          "slug": "sm",
          "size": "0.875rem",
          "name": "Small"
        },
        {
          "slug": "md",
          "size": "1rem",
          "name": "Medium"
        },
        {
          "slug": "h6",
          "size": "1.125rem",
          "name": "H6"
        },
        {
          "slug": "h5",
          "size": "1.375rem",
          "name": "H5"
        },
        {
          "slug": "h4",
          "size": "2.125rem",
          "fluid": {
            "min": "1.375rem",
            "max": "2.125rem"
          },
          "name": "H4"
        },
        {
          "slug": "h3",
          "size": "2.5rem",
          "fluid": {
            "min": "2.125rem",
            "max": "2.5rem"
          },
          "name": "H3"
        },
        {
          "slug": "h2",
          "size": "3.375rem",
          "fluid": {
            "min": "2.5rem",
            "max": "3.375rem"
          },
          "name": "H2"
        },
        {
          "slug": "h1",
          "size": "4.5rem",
          "fluid": {
            "min": "3.375rem",
            "max": "4.5rem"
          },
          "name": "H1"
        }
      ],
      "fontWeight": true,
      "fluid": true
    },
    "spacing": {
      "units": ["px", "em", "rem", "vh", "vw", "%"],
      "padding": true,
      "margin": true
    },
    "layout": { 
      "contentSize": "1168px", 
      "wideSize": "1268px" 
    },
    "custom": {
      "fontWeight": {
        "extraBold": "700",
        "bold": "600",
        "medium": "500",
        "regular": "400"
      },
      "lineHeight": {
        "flat": "1",
        "normal": "1.25",
        "relaxed": "1.5",
        "loose": "1.625"
      },
      "container": {
        "sm": "980px",
        "md": "1200px",
        "lg": "1300px"
      },
      "borderRadius": {
        "xs": "8px",
        "sm": "12px",
        "md": "16px",
        "lg": "24px",
        "xl": "40px"
      },
      "transition": {
        "base": "all 0.3s ease"
      },
      "border": {
        "base": "1px solid var(--wp--preset--color--black)",
        "transparent": "1px solid transparent"
      },
      "shadow": {
        "base": "rgba(99, 99, 99, 0.2) 0px 2px 8px 0px"
      }
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--light)",
      "text": "var(--wp--preset--color--black)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--secondary)",
      "fontSize": "var(--wp--preset--font-size--md)",
      "fontWeight": "var(--wp--custom--font-weight--regular)",
      "lineHeight": "var(--wp--custom--line-height--loose)"
    },
    "spacing": {
      "margin": {
        "top": "0",
        "right": "0",
        "bottom": "0",
        "left": "0"
      }
    },
    "elements": {
      "link": {
        "color": {
          "text": "inherit"
        },
        "typography": {
          "textDecoration": "none"
        },
        ":hover": {
          "typography": {
            "textDecoration": "none"
          }
        }
      },
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--primary)",
          "fontSize": "var(--wp--preset--font-size--h-1)",
          "fontWeight": "var(--wp--custom--font-weight--bold)",
          "lineHeight": "var(--wp--custom--line-height--flat)"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--primary)",
          "fontSize": "var(--wp--preset--font-size--h-2)",
          "fontWeight": "var(--wp--custom--font-weight--bold)",
          "lineHeight": "var(--wp--custom--line-height--flat)"
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--primary)",
          "fontSize": "var(--wp--preset--font-size--h-3)",
          "fontWeight": "var(--wp--custom--font-weight--bold)",
          "lineHeight": "var(--wp--custom--line-height--flat)"
        }
      },
      "h4": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--primary)",
          "fontSize": "var(--wp--preset--font-size--h-4)",
          "fontWeight": "var(--wp--custom--font-weight--bold)",
          "lineHeight": "var(--wp--custom--line-height--flat)"
        }
      },
      "h5": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--primary)",
          "fontSize": "var(--wp--preset--font-size--h-5)",
          "fontWeight": "var(--wp--custom--font-weight--bold)",
          "lineHeight": "var(--wp--custom--line-height--flat)"
        }
      },
      "h6": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--primary)",
          "fontSize": "var(--wp--preset--font-size--h-6)",
          "fontWeight": "var(--wp--custom--font-weight--bold)",
          "lineHeight": "var(--wp--custom--line-height--flat)"
        }
      }
    }
  }
}
